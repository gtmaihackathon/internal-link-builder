# Streamlit Secrets Example
# Copy this to .streamlit/secrets.toml (DO NOT commit to git!)
# Or add these in Streamlit Cloud dashboard under "Secrets"

# Example API Keys (if needed in future)
# ANTHROPIC_API_KEY = "your-anthropic-api-key"
# OPENAI_API_KEY = "your-openai-api-key"

# Database credentials (if using external DB)
# [database]
# host = "your-db-host.com"
# port = 5432
# username = "your-username"
# password = "your-password"
# database = "internal_links"

# Google Search Console API (future feature)
# [google]
# client_id = "your-client-id"
# client_secret = "your-client-secret"
